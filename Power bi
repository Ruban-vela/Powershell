
Week Range = 
VAR WeekStart = 'YourTable'[DateColumn] - WEEKDAY('YourTable'[DateColumn], 2) + 1
VAR WeekEnd = WeekStart + 6
RETURN
FORMAT(WeekStart, "MMM dd") & " - " & FORMAT(WeekEnd, "MMM dd")



WeekSortKey = 
VAR WStart = 'YourTable'[DateColumn] - WEEKDAY('YourTable'[DateColumn], 2) + 1
RETURN 
YEAR(WStart) * 10000 + MONTH(WStart) * 100 + DAY(WStart)
